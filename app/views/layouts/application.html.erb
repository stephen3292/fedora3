<!DOCTYPE html>
<html>
<head>
  <title>MyFedora</title>
    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
</head>
<body>


<% if signed_in? %>
  <%= current_user.username %>

  <%= current_user.description %><%= button_to "Sign Out", session_url, method: :delete %>
<% else %>
  <%= "Fedora!" %>
  <%= link_to "Sign Up", new_user_url %>
  <%= link_to "Sign In", new_session_url %>
<% end %>

<%= yield %>

</body>
</html>
